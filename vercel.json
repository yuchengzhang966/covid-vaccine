{
  "version": 2,
  "name": "covid-vaccine-dashboard",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "main.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/data/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/main.js",
      "dest": "/main.js"
    },
    {
      "src": "/src/data/(.*)",
      "dest": "/src/data/$1"
    }
  ]
}
